{
	"name": "biota",
	"displayName": "Biota",
	"description": "Anti BIOME",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.98.0"
	},
	"categories": ["Other"],
	"activationEvents": ["onLanguage:javascript", "onLanguage:typescript"],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "biota.formatCode",
				"title": "Biota: Format Code"
			},
			{
				"command": "biota.lintCode",
				"title": "Biota: Lint Code"
			},

			{
				"command": "biota.fakeErrors",
				"title": "Biota: Random Extension Errors"
			},
			{
				"command": "biota.breakColors",
				"title": "Biota: Syntax Highlighting Sabotage"
			},
			{
				"command": "biota.hotReload",
				"title": "Biota: Hot Reload"
			},

			{
				"command": "biota.breakGit",
				"title": "Biota: Fix version control"
			}
		],
		"languages": [
			{
				"id": "javascript",
				"extensions": [".js"]
			},
			{
				"id": "typescript",
				"extensions": [".ts"]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.98.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@typescript-eslint/eslint-plugin": "^8.25.0",
		"@typescript-eslint/parser": "^8.25.0",
		"eslint": "^9.21.0",
		"typescript": "^5.7.3",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1"
	}
}
